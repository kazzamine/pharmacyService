{% for familleitem in famille %}
	<div
		class="card sideCat justify-content-center align-items-center" id="{{familleitem.id}}">
		{# <img src="{{ asset('img/vacc.png') }}" class="card-img-cat" alt="..."> #}
		{% set famillePic = 'Image-Not-Available.png' %}
		{% if familleitem.image %}
			{% set famillePic = familleitem.image %}
		{% endif %}
		<img class="icon-gestion mt-1" src="{{ asset('img/' ~ famillePic) }}" width="90%" height='50px'>
	</img>
	<div class="card-body famille-card">
		<h5 class="card-title cat-title" id="{{familleitem.id}}">{{familleitem.designation}}
		</h5>
	</div>
</div>
{% endfor %}
